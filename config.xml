<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.vineui776385" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>smartquibdo</name>
  <description>
        Vive al dia con las noticias del choc√≥
    </description>
  <author email="sigtics@gmail.com" href="http://sigtics.org/">
      Sigtics
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="phonegap-version" value="3.7.0" />
